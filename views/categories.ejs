<h2>Categories</h2>
<a href="/categories">Categories</a>
<% console.log('categories --->', categories) %>
    <ul class="list-group">
        <% for( let i=0; i < categories.length; i++ ) { %>
            <% let category=categories[i] %>
                <li class="list-group-item">
                    <a href="/categories/<%= category.id %>">
                        <%= category.category_name %>
                    </a>
                    <% } %>
                </li>
    </ul>