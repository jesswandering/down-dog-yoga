<h2>Yoga Poses</h2>
<ul>
    <% for( let i=0; i < poses.length; i++ ) { %>
        <% let pose=poses[i] %>
            <img src="<%= pose.img %>" alt="">
            <li class="sidelist">
                <p>
                    <%= pose.name %>
                </p>
                <p>
                    <%= pose.description %>
                </p>
                <% } %>
            </li>
</ul>